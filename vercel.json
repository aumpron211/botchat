{
  "version": 2,
  "name": "botchat",
  "alias": ["Botchat"],
  "builds": [{ "src": "app.js", "use": "@vercel/node" }],
  "routes": [{
    "headers": {
        "Access-Control-Allow-Origin": "*",
        "Access-Control-Allow-Methods": "GET, POST, PUT, DELETE, OPTIONS",
    },
    "src": "/.*", "dest": "app.js" }
  ]
